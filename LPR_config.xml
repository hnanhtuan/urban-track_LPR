<?xml version="1.0"?>
<opencv_storage>

<!-- cascade_classifier -->
<box_cascade_classifier>cascade_classifier/cascade_box.xml</box_cascade_classifier>
<long_cascade_classifier>cascade_classifier/cascade_long.xml</long_cascade_classifier>
<filter_by_size>true</filter_by_size>
<tripple_check>false</tripple_check>
<lp_detect_area_height>360</lp_detect_area_height>
<min_box>58 35</min_box>
<max_box>150 90</max_box>
<min_long>113 20</min_long>
<max_long>225 50</max_long>

<text_box_resize>200</text_box_resize>
<box_min_text_height>0.2</box_min_text_height>
<box_max_text_height>0.45</box_max_text_height>
<long_min_text_height>0.4</long_min_text_height>
<long_max_text_height>0.7</long_max_text_height>
<long_ideal_text_height>0.48</long_ideal_text_height>

<min_text_ratio>0.3</min_text_ratio>
<max_text_ratio>1.8</max_text_ratio>
<min_I_text_ratio>3.2</min_I_text_ratio>
<max_I_text_ratio>4.7</max_I_text_ratio>
<letter_classifier_param_file>letter_classifier/letter_classifier_params.xml</letter_classifier_param_file>
<lp_color_classifier_param_file>lp_color_classifier/lp_color_classifier_params.xml</lp_color_classifier_param_file>

<!-- LP filter by color -->
<extend_area>0.1</extend_area>
<threshold>0.4</threshold>
<num_cols>20</num_cols>
<num_rows>20</num_rows>

<yellow_lower_range> 80.  25.  55. 0.</yellow_lower_range>
<yellow_upper_range>105. 220. 255. 0.</yellow_upper_range>

<red_lower_range>110.  25.  55. 0.</red_lower_range> 
<red_upper_range>130. 220. 255. 0.</red_upper_range> 

<black_lower_range>  0.   0.   0. 0.</black_lower_range>
<black_upper_range>179. 150. 150. 0.</black_upper_range>

<white_lower_range>  0.   0. 150. 0.</white_lower_range> 
<white_upper_range>179. 150. 255. 0.</white_upper_range> 

</opencv_storage>
